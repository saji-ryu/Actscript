{
  "manifest_version": 2,
  "name": "SensorExtension",
  "description": "extension that logs sensor-data",
  "version": "0.0.4",
  "icons": {
    "48": "/icons/icon.png"
  },
  "permissions": [
    "notifications",
    "storage",
    "*://new-linda.herokuapp.com/*",
    "webRequest",
    "geolocation",
    "webNavigation"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_specific_settings": {
    "gecko": {
      "strict_min_version": "66.0",
      "update_url": "https://raw.githubusercontent.com/saji-ryu/Actscript/master/modules/sensorExtension/updates.json"
    }
  }
}
